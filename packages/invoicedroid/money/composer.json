{
    "name":"invoicedroid/money",
    "description":"Currency formatting and conversion package.",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "keywords":[
        "laravel",
        "currency",
        "money"
    ],
    "license":"MIT",
    "authors":[
        {
            "name":"Goran Krgovic",
            "email":"info@invoicedroid.com",
            "homepage":"https://www.invoicedroid.org",
            "role":"Developer"
        }
    ],
    "require":{
        "php":">=5.5.9",
        "illuminate/support":">=5.8 <6.0",
        "illuminate/view":">=5.8 <6.0"
    },
    "require-dev":{
        "mockery/mockery":"~0.9",
        "phpunit/phpunit":"~4.0"
    },
    "autoload":{
        "psr-4":{
            "InvoiceDroid\\Money\\":"./src"
        },
        "files":[
            "src/helpers.php"
        ]
    },
    "extra":{
        "laravel":{
            "providers":[
                "InvoiceDroid\\Money\\ServiceProvider"
            ]
        }
    }
}
